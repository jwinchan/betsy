<section>
  <%= form_with model: @product do |f| %>
    <div class="form-group">
      <%= f.label :rating %>
      <%= f.text_field :rating%>
    </div>
    <div class="form-group">
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
    <div class="form-group">
      <%= f.submit "submit_button", class: "btn btn-primary" %>
    </div>
  <% end %>
</section>